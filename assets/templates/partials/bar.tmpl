<div class="wrapper">
    <form class="col-wrap search__form" action="/search">
        <label class="search__label">Search for data and analysis</label>
        <input type="search" autocomplete="off" class="search__input search__input--results-page col col--md-23 col--lg-32 js-auto-submit__input" id="nav-search" name="q" value={{ .Data.Query}}>
        <button type="submit" class="search__button search__button--results-page col--md-3 col--lg-3" id="nav-search-submit">
            <span class="visuallyhidden">Search</span>
            <span class="icon icon-search--light"></span>
        </button>
    </form>
</div> 